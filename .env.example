# ============================================
# ClawAI King Booster - Environment Variables
# ============================================
# Copy this file to .env.local and fill in your values
# NEVER commit .env.local to git!

# ============================================
# OPENCLAW
# ============================================
OPENCLAW_LOCAL_KEY=your_openclaw_local_key

# ============================================
# WALLET ADDRESSES
# ============================================
ZORA_PUBLIC_WALLET=0x_your_public_wallet_address

# ============================================
# BASE RPC
# ============================================
BASE_RPC=https://base-mainnet.core.chainstack.com/your_api_key
BASE_WSS=wss://base-mainnet.core.chainstack.com/your_api_key
BASE_RPC_USER=your_rpc_username
BASE_RPC_PASS=your_rpc_password

# ============================================
# ZORA RPC
# ============================================
ZORA_RPC=https://zora-mainnet.core.chainstack.com/your_api_key
ZORA_WSS=wss://zora-mainnet.core.chainstack.com/your_api_key
ZORA_RPC_USER=your_rpc_username
ZORA_RPC_PASS=your_rpc_password

# ============================================
# SUPABASE (Required)
# ============================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# PostgreSQL Direct Connection
DATABASE_URL=postgresql://postgres:your_password@db.your-project.supabase.co:5432/postgres

# ============================================
# NEYNAR / FARCASTER (Required)
# ============================================
NEYNAR_API_KEY=your_neynar_api_key
NEYNAR_CLIENT_ID=your_neynar_client_id
NEYNAR_SNAPCHAIN_HTTPS=https://snapchain-api.neynar.com
NEYNAR_SNAPCHAIN_GRPC=snapchain-grpc-api.neynar.com:443
NEYNAR_WEBHOOK_URL=https://api.neynar.com/f/app/your_client_id/event

# ============================================
# ZSIGNALS API
# ============================================
ZSIGNALS_API_KEY=your_zsignals_api_key

# ============================================
# AI PROVIDERS (Fallback Chain)
# At least one required for bot functionality
# ============================================
# Primary
GEMINI_API_KEY=your_gemini_api_key

# Fallbacks (in order)
GROQ_API_KEY=your_groq_api_key
GROQ_ORG_ID=your_groq_org_id

MISTRAL_API_KEY=your_mistral_api_key
MISTRAL_AGENT_ID=your_mistral_agent_id

APIFREE_API_KEY=your_apifree_api_key
AIMLAPI_API_KEY=your_aimlapi_api_key

# ============================================
# MARKET DATA
# ============================================
ALPHAVANTAGE_API_KEY=your_alphavantage_api_key
GECKOTERMINAL_BASE_URL=https://api.geckoterminal.com/api/v2

# ============================================
# WALLET CONNECT
# ============================================
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# ============================================
# APP CONFIG
# ============================================
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=ClawAI King Booster

# ============================================
# INTERNAL KEYS
# ============================================
CLAW_BOOSTER_KEY=your_internal_booster_key
JWT_SECRET=your_jwt_secret
